{
	"manifest_version": 3,
	"name": "test",
	"description": "test",
	"version": "0.0.1",
	"background": {
		"service_worker": "/background.js"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["/content.js"]
		}
	],
	"permissions": [
		"tabs",
		"scripting",
		"storage"
	],
	"host_permissions": [
        "https://orioks.miet.ru/*"
    ]
}
