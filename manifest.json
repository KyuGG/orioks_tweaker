{
    "manifest_version": 2,
    "name": "orioks_tweaker",
    "version": "0.9",
    "browser_action": {
        "default_title": "ORIOKS tweaker",
        "default_icon": "interface/logo.png",
        "default_popup": "interface/interface.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://orioks.miet.ru/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "activeTab"
    ]
}